web: java $JAVA_OPTS -DCONSTRETTO_TAGS=production -Ddw.database.url=$JDBC_DATABASE_URL -jar target/polet-0.1.0-SNAPSHOT.jar db migrate config.yml && java $JAVA_OPTS -Ddw.database.url=$JDBC_DATABASE_URL -Ddw.server.connector.port=$PORT -DCONSTRETTO_TAGS=production -jar target/polet-0.1.0-SNAPSHOT.jar server config.yml