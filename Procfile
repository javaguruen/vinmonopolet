web: java $JAVA_OPTS -DCONSTRETTO_TAGS=production -Ddw.database.url=$JDBC_DATABASE_URL -jar backend/target/polet-backend-0.2.0-SNAPSHOT.jar db migrate backend/config.yml && java $JAVA_OPTS -Ddw.database.url=$JDBC_DATABASE_URL -Ddw.server.connector.port=$PORT -DCONSTRETTO_TAGS=production -jar backend/target/polet-backend-0.2.0-SNAPSHOT.jar server backend/config.yml